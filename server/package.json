{
  "name": "diary",
  "version": "1.0.0",
  "description": "A diary app ",
  "main": "app.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "nodemon --exec babel-node app.js",
    "test": "nyc mocha --require @babel/register --exit",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov | coveralls"
  },
  "author": "Nasri",
  "license": "ISC",
  "dependencies": {
    "chai-http": "^4.2.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.0",
    "nyc": "^13.3.0"
  }
}
